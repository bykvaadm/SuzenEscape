else
echo 'Git is a free and open source distributed version control system designed to handle everything from small to very large projects with speed and efficiency. ' $i > readme.txt
fi
git add .
git commit -m "Index"
done
    echo 'FLAG IS ' $2 > readme.txt 
    git add . 
    git commit -m "Index"
